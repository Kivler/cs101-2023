#include <stdio.h>

int main()
{
    int i = 0, num = 0;
    char a[] = "AABBBCCCCddd";
    char c = a[0];
    printf("%c", a[0]);
    while(c != '\0'){
        if(c != a[i]){
            printf("%d", num);
            num = 0;
            c = a[i];
            printf("%c", c+1);
        }else{
            num++ ;
        }
        
        i++;
    }

    return 0;
}
